@model Dictionary<int, ContactsApp.Models.Contact>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<table class="table table-hover">
    <tr>
        <th>First name</th>
        <th>Last name </th>
        <th>Email</th>
        <th>Phone number</th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Value.FirstName</td>
            <td>@item.Value.LastName</td>
            <td>@item.Value.Email</td>
            <td>@item.Value.Phone</td>
            <td>@Html.ActionLink("edit", "Edit", new { id = item.Key })</td>
            <td>@Html.ActionLink("delete", "Delete", new { id = item.Key })</td>
        </tr>

    }
</table>
